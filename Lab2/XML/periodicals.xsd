<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="periodicals">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="booklet" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="id" type="xs:string"/>
                            <xs:element name="isMulticolor" type="xs:boolean"/>
                            <xs:element name="size" type="xs:integer"/>
                            <xs:element name="title" type="xs:string"/>
                            <xs:element name="isMonthly" type="xs:boolean"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="newspaper" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="id" type="xs:string"/>
                            <xs:element name="isMulticolor" type="xs:boolean"/>
                            <xs:element name="size" type="xs:integer"/>
                            <xs:element name="title" type="xs:string"/>
                            <xs:element name="isDaily" type="xs:boolean"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="journal" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="id" type="xs:string"/>
                            <xs:element name="isMulticolor" type="xs:boolean"/>
                            <xs:element name="size" type="xs:integer"/>
                            <xs:element name="title" type="xs:string"/>
                            <xs:element name="isMonthly" type="xs:boolean"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>
